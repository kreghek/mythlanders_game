{
  "stryker-config": {
    "mutation-level": "Basic",
    "ignore-methods": [
      "*Log",
      "Console.Write*",
      "*Exception.ctor",
      "ToString",
      "ConfigureAwait"
    ],
    "ignore-mutations": [
      "string",
      "linq.First",
      "linq.Sum",
      "statement",
      "block"
    ],
    "mutator": {
      "exclude-mutations": [
        "StringLiteral",
        "BlockStatement",
        "string",
        "statement",
        "block"
      ]
    }
  }
}